pipeline{
    agent any
    stages{
 
        stage("Git Checkout "){
            steps{
                git branch: 'main',
                credentialsId: '6bc8f840-c1ae-4a4d-aea0-af92ab6f29f7',
                url: 'git@github.com:rupinder-on-github/knowledgetub2-ec2.git'
            }
            post{
                always{
                    echo "========always========"
                }
                success{
                    echo "========A executed successfully========"
                }
                failure{
                    echo "========A execution failed========"
                }
            }
        }
    }
    post{
        always{
            echo "========always========"
        }
        success{
            echo "========pipeline executed successfully ========"
        }
        failure{
            echo "========pipeline execution failed========"
        }
    }
}